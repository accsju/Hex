{
    "name": "index",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.cjs",
    "module": "dist/index.esm.js",
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.esm.js"
        }
    },
    "scripts": {
        "build": "npm run build:esm && npm run build:cjs",
        "build:esm": "esbuild src/index.js --bundle --format=esm --outfile=dist/index.esm.js",
        "build:cjs": "esbuild src/index.js --bundle --format=cjs --outfile=dist/index.cjs"
    },
    "keywords": [
        "javascript",
        "library",
        "utility",
        "index"
    ],
    "author": "kai",
    "license": "MIT",
    "devDependencies": {
        "esbuild": "^0.25.3"
    }
}
